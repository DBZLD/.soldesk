package extend;

public class Main {
	public static void main(String[] args) {
		GameObject go = new GameObject();
		go.stName = "게임오브젝트";
		Character ch = new Character();
		ch.stName = "캐릭터";
		ch.nHp = 10;
		Item i = new Item();
		i.stName = "아이템";
		i.nPrice = 100;
		Sword s = new Sword();
		s.stName = "검";
		s.nPrice = 500;
		s.nDamage = 1;

		go.Info();
		ch.Info();
		i.Info();
		s.Info();
	}
}
