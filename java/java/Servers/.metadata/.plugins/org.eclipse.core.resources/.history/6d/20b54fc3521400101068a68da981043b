package kioskV3;

public class Product {
	String stName;
	int nPrice;
	
	public void Info() {
		System.out.println(stName + "(" + nPrice + "Ïõê)");
	}
	Product(String stName, int nPrice){
		this.stName = stName;
		this.nPrice = nPrice;
	}
	Product(int nPrice, String stName){
		this.nPrice = nPrice;
		this.stName = stName;
	}
}
